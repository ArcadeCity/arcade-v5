{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "noEmit": true,
    "outDir": "dist",
    "rootDir": "src",
    "typeRoots": ["../../@types", "../../node_modules/@types"],
    "module": "esnext",
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src/*"],
  "exclude": ["src/setupProxy.js", "node_modules", "dist"],
  "references": [
    {
      "path": "../components"
    }
  ]
}
